+import ExpoModulesCore
+import Foundation
+
+public protocol NotificationsPresentationDelegate {
+  func willPresent(_ notification: UNNotification, completionHandler: @escaping (UNNotificationPresentationOptions) -> Void)
+}
+
+public protocol NotificationsResponseDelegate {
+  func didReceive(_ response: UNNotificationResponse, completionHandler: @escaping () -> Void)
+}
+public protocol NotificationsSettingsDelegate {
+  func openSettings(_ notification: UNNotification?)
+}
+
+class NotificationsListener: Equatable, Hashable {
+  static func == (lhs: NotificationsListener, rhs: NotificationsListener) -> Bool {
+    return lhs.identifier == rhs.identifier
+  }
+  
+  let identifier: String = UUID().uuidString
+  let delegate: AnyObject?
+
+  init (delegate: AnyObject) {
+    self.delegate = delegate
+  }
+
+  func hash(into hasher: inout Hasher) {
+    hasher.combine(identifier.hashValue)
+  }
+
+}
+
+public class NotificationCenterDelegate: NSObject, UNUserNotificationCenterDelegate {
+  public static let shared = NotificationCenterDelegate()
+
+  var listeners: Set<NotificationsListener> = []
+  var pendingResponses: [UNNotificationResponse] = []
+  let userNotificationCenter: UNUserNotificationCenter = UNUserNotificationCenter.current()
+
+  private override init() {
+    super.init()
+    if UNUserNotificationCenter.current().delegate != nil {
+      NSLog("[expo-notifications] EXNotificationCenterDelegate encountered already present delegate of UNUserNotificationCenter. EXNotificationCenterDelegate will not overwrite the value not to break other features of your app.  In return, expo-notifications may not work properly.  To fix this problem either remove setting of the second delegate  or set the delegate to an instance of EXNotificationCenterDelegate manually afterwards.")
+      return
+    }
+    UNUserNotificationCenter.current().delegate = self
+  }
+
+  public func addDelegate(_ delegate: AnyObject) {
+    listeners.insert(NotificationsListener(delegate: delegate))
+    if let delegate = delegate as? NotificationsResponseDelegate {
+      for pendingResponse in pendingResponses {
+        delegate.didReceive(pendingResponse, completionHandler: {})
+      }
+      pendingResponses.removeAll()
+    }
+  }
+
+  @objc public func removeDelegate(_ delegate: AnyObject) {
+    var listenerToRemove: NotificationsListener?
+    for listener in listeners {
+      if listener.delegate === delegate {
+        listenerToRemove = listener
+      }
+    }
+    if let listenerToRemove = listenerToRemove {
+      listeners.remove(listenerToRemove)
+    }
+  }
+
+  // MARK: - UNUserNotificationCenterDelegate
+
+  public func userNotificationCenter(
+    _ center: UNUserNotificationCenter,
+    willPresent notification: UNNotification,
+    withCompletionHandler completionHandler: @escaping (UNNotificationPresentationOptions) -> Void
+  ) {
+    for listener in listeners {
+      if let delegate = listener.delegate as? NotificationsPresentationDelegate {
+        delegate.willPresent(notification, completionHandler: completionHandler)
+      }
+    }
+    completionHandler([])
+  }
+
+  public func userNotificationCenter(_ center: UNUserNotificationCenter, didReceive response: UNNotificationResponse, withCompletionHandler completionHandler: @escaping () -> Void) {
+    var handled = false
+    for listener in listeners {
+      if let delegate = listener.delegate as? NotificationsResponseDelegate {
+        delegate.didReceive(response, completionHandler: completionHandler)
+      }
+      handled = true
+    }
+    if !handled {
+      pendingResponses.append(response)
+    }
+    completionHandler()
+  }
+
+  public func userNotificationCenter(_ center: UNUserNotificationCenter, openSettingsFor notification: UNNotification?) {
+    for listener in listeners {
+      if let delegate = listener.delegate as? NotificationsSettingsDelegate {
+        delegate.openSettings(notification)
+      }
+    }
+  }
+}
